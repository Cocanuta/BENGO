{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["main.js", "./libs", "package.json", "README.md"],
    "includePattern": ".+\\.(js|jsx)?$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "parser": "gfm",
    "hardwrap": true,
    "excludeTags": []
  },
  "templates": {
    "systemName": "Bengo",
    "systemSummary": "The server backend for the E3 2019 Twitch extension.",
    "systemLogo": "./docs/logo.png",
    "systemColor": "#2AA226",
    "copyright": "SnatchyBuckles Â© 2019",
    "linenums": true
  },
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "private": false,
    "lenient": true,
    "destination": "./docs",
    "template": "./node_modules/foodoc/template"
  }
}
